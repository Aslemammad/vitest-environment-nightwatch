{
  "name": "vitest-environment-nightwatch",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "publish": "npm run build && clean-publish",
    "release": "bumpp package.json --commit --push --tag && npm run publish",
    "test": "vitest --no-threads"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs",
      "default": "./dist/types.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinylibs/tinyspy.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tinylibs/tinyspy/issues"
  },
  "homepage": "https://github.com/tinylibs/tinyspy#readme",
  "devDependencies": {
    "@nightwatch/testing-library": "^0.1.1",
    "@nightwatch/vue": "^0.4.1",
    "@types/nightwatch": "^2.3.18",
    "@typescript-eslint/parser": "^5.53.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/ui": "^0.27.1",
    "chromedriver": "^109.0.0",
    "eslint": "^8.34.0",
    "nightwatch": "^2.6.13",
    "ts-node": "^10.9.1",
    "tsup": "^6.6.3",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-nightwatch": "^0.4.2",
    "vitest": "^0.27.1",
    "vue-tsc": "^1.0.11"
  },
  "peerDependencies": {
    "nightwatch": "*",
    "vitest": "*"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "browser": false,
      "node": true
    },
    "rules": {
      "eqeqeq": "off",
      "curly": [
        "error",
        "all"
      ],
      "quotes": [
        "error",
        "single"
      ]
    }
  }
}
